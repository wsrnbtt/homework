#include <stdio.h>

int *GetSet(int *num);

int main()
{
    int *data;
    int num;

    data = GetSet(&num);

    printf("N: %d\n", num);

    if(num > 0)
    {
        for (int i = 0; i < num; i++)
        {
            printf("%d", data[i]);
        }
        printf("\n");
    }

    return 0;
}

int *GetSet(int *num)
{
    static int arr[20];
    int n;

    printf("input N (max20): ");

    if(scanf("%d",&n) != 1 || n <= 0 || n>20)
    {
        *num = 0;
        return NULL;
    }

    printf("input %d integers:\n",n);

    for(int i =0; i<n; i++)
    {
        if(scanf("%d",&arr[i]) != 1)
        {
            n = i ;
            break;
        }
    }

    *num = n;

    return arr;
}

